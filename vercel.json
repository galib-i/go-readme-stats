{
  "github": {
    "silent": true
  },
  "git": {
    "deploymentEnabled": false
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/galib-i/go-readme-stats",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/vercel.go"
    }
  ]
}